public class Converter 
{
	private double converstionFactor;
	
	public Converter(double aConverstionFactor)
	{
		converstionFactor = aConverstionFactor;
	}
	
	/**
	 * 
	 * @param fromMeasurement: type in the amount of meters
	 * @return meters in the factor you want it in
	 */
	public double convertTo(double fromMeasurement)
	{
		double convertednumber = fromMeasurement * converstionFactor;
		return convertednumber;
	}
	
	public static final double METERS_TO_MILES = 1.609;
	public static final double METERS_TO_FEET = 3.28084;
	public static final double METERS_TO_INCHES = 39.3701;
}
