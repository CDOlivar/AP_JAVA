public class Cards 
public class Cards 
	{
		public Cards()
			{
			
			}
	
		
		
		public static String analyze(String firstString) //prints Card Values
			{	
							
				
				if(firstString.equalsIgnoreCase("1") || firstString.equalsIgnoreCase("2") || firstString.equalsIgnoreCase("3") || firstString.equalsIgnoreCase("4") || firstString.equalsIgnoreCase("5") || firstString.equalsIgnoreCase("6") || firstString.equalsIgnoreCase("7") || firstString.equalsIgnoreCase("8") || firstString.equalsIgnoreCase("9") || firstString.equalsIgnoreCase("10"))
						{
						return firstString;
					}
				else if(firstString.equalsIgnoreCase("a") )
				{
					return "Ace";
				}
			else if(firstString.equalsIgnoreCase("a"))
				{
					return "Jack";
				}
			else if(firstString.equalsIgnoreCase("k"))
				{
					return "King";
				}
			else if(firstString.equalsIgnoreCase("q"))
				{
					return "Queen";
				}
			else if(firstString.equalsIgnoreCase("d") )
				{
					return "Diamonds";
				}
			else if(firstString.equalsIgnoreCase("h"))
				{
					return "Hearts";
				}
			else if(firstString.equalsIgnoreCase("s"))
			{
					return "Spades";
				}
			else if(firstString.equalsIgnoreCase("c"))
				{
					return "Clubs";
				}	
				
				return "Unreadable Value";
				
			}
		
	}
